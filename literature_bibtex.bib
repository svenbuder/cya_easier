	date-added = {2024-07-04 11:53:55 +1000},
	date-modified = {2024-07-04 11:53:55 +1000},
	doi = {10.1051/0004-6361:200809698},
	eprint = {0805.4826},
	journal = {\aap},
	keywords = {methods: data analysis, techniques: spectroscopic, stars: fundamental parameters, stars: planetary systems, stars: planetary systems: formation, Galaxy: solar neighborhood},
	month = aug,
	pages = {373-381},
	title = {{Spectroscopic parameters for 451 stars in the HARPS GTO planet search program. Stellar [Fe/H] and the frequency of exo-Neptunes}},
	volume = 487,
	year = 2008,
